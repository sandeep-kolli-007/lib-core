{
  "name": "tms-react-ui",
  "version": "1.0.0",
  "description": "sharing UI library",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "babel src --watch --out-dir lib --extensions .ts,.tsx --source-maps inline --copy-files",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib --extensions .ts,.tsx --source-maps inline --copy-files && tsc --declaration --emitDeclarationOnly",
    "tsc": "tsc --watch --declaration --emitDeclarationOnly"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.6.4",
    "jest": "^29.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "react-router-dom": "^6.15.0",
    "typescript": "^5.2.2"
  }
}
