{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "jsx": "react",
      "declaration": true,
      "outDir": "./lib",
      "removeComments": true,
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "allowJs": true,
      "noImplicitThis": false,
      "skipLibCheck": true,
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ]
    },
    "files": [
      "custom.d.ts"
    ],
    "include": [
      "src",
      "custom.d.ts"
    ]
  }